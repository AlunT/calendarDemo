<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="bgColor_overlay">#60000000</color>
    <color name="pickervier_bottom_bg">#B7B7B7</color>
    <color name="pickerview_bg_topbar">#f5f5f5</color>
    <color name="pickerview_timebtn_nor">#057dff</color>
    <color name="pickerview_timebtn_pre">#c2daf5</color>
    <color name="pickerview_topbar_title">#000000</color>
    <color name="pickerview_wheelview_textcolor_center">#2a2a2a</color>
    <color name="pickerview_wheelview_textcolor_divider">#d5d5d5</color>
    <color name="pickerview_wheelview_textcolor_out">#a8a8a8</color>
    <color name="pickerview_white">#FFFFFF</color>
    <declare-styleable name="switch_button"><attr format="string" name="switch_left_text"/><attr format="string" name="switch_right_text"/><attr format="dimension" name="switch_text_size"/><attr format="color" name="select_text_color"/><attr format="color" name="unselect_text_color"/><attr format="color" name="select_text_color_bg"/><attr format="color" name="unselect_text_color_bg"/><attr name="select_which">
            <enum name="select_left" value="0"/>
            <enum name="select_right" value="1"/>
        </attr></declare-styleable>
    <declare-styleable name="wheelview"><attr name="gravity">
            <enum name="center" value="17"/>
            <enum name="left" value="3"/>
            <enum name="right" value="5"/>
        </attr><attr format="color" name="textColorOut"/><attr format="color" name="textColorCenter"/><attr format="color" name="dividerColor"/></declare-styleable>
    <dimen name="pickerver_height_margin">80dp</dimen>
    <dimen name="pickerview_border">1dp</dimen>
    <dimen name="pickerview_switch_height">35dp</dimen>
    <dimen name="pickerview_switch_radius">3dp</dimen>
    <dimen name="pickerview_switch_text_radius">2dp</dimen>
    <dimen name="pickerview_switch_text_size">15dp</dimen>
    <dimen name="pickerview_switch_width">100dp</dimen>
    <dimen name="pickerview_topbar_btn_textsize">16dp</dimen>
    <dimen name="pickerview_topbar_height">44dp</dimen>
    <dimen name="pickerview_topbar_paddingleft">15dp</dimen>
    <dimen name="pickerview_topbar_paddingright">15dp</dimen>
    <dimen name="pickerview_topbar_title_textsize">18dp</dimen>
    <integer name="animation_default_duration">300</integer>
    <string name="height_cm">CM</string>
    <string name="height_in">IN</string>
    <string name="pickerview_cancel">取消</string>
    <string name="pickerview_day">日</string>
    <string name="pickerview_hours">时</string>
    <string name="pickerview_minutes">分</string>
    <string name="pickerview_month">月</string>
    <string name="pickerview_seconds">秒</string>
    <string name="pickerview_submit">确定</string>
    <string name="pickerview_year">年</string>
    <string name="weight_kg">KG</string>
    <string name="weight_lbs">LB</string>
    <style name="AnimBottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/slide_in_bottom</item>
        <item name="android:windowExitAnimation">@anim/slide_out_bottom</item>
    </style>
    <style name="Dialog_Fullscreen">
        <item name="android:windowNoTitle">true</item>
    </style>
</resources>